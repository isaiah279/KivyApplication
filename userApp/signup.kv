<Signup>:
    name:"signup"
    MDLabel:
        text:"signup "
        pos_hint_y:{"center_y":.95}
        font_style:"H4"
        halign:"center"
        theme_text_color:"Custom"
        text_color:0,0,1,1
    MDTextField:
        id:email
        hint_text:"Enter Your Email"
        pos_hint:{"center_x":.5,"center_y":.75}
        current_hint_text_color:.0,0,0,1
        size_hint_x:.8
    MDTextField:
        id:password
        hint_text:"Password"
        pos_hint:{"center_x":.5,"center_y":.55}
        current_hint_text_color:.0,0,0,1
        size_hint_x:.8
        md_bg_text_color:0,0,0,0
        password:True
    MDRaisedButton:
        text:"Sign Up"
        pos_hint:{"center_x":.5,"center_y":.30}
        size_hint_x:.5
        theme_text_color:"Custom"
        text_color:0,0,0,1
        on_press:app.signing_up( email.text,password.text)
        on_press:app.change_screen("signup")
    MDRaisedButton:
        text:"Log in"
        pos_hint:{"center_x":.5,"center_y":.20}
        size_hint_x:.5
        theme_text_color:"Custom"
        text_color:0,0,0,1
        on_press:app.change_screen("login")
